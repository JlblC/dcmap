iterate_all players
{
	$dip == 'Мульт' ?: continue;

    echo '--------------------------------------------';
    echo "player: $name";

	my $pid = $name;
	iterate_all fleets
	{
		if($type != 2 or $owner != $pid or $deleted or $outdated) continue;

		echo "  $name at $x:$y";
	};
};

















